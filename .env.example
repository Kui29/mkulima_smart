# Example .env file

# Secret Key for Flask sessions (replace in actual .env)
SECRET_KEY=your-secret-key

# Database Configuration (either use DATABASE_URL or individual parts)
DATABASE_URL=postgresql://username:password@localhost:5432/mkulima_smart

# Or specify individual DB parts (used if DATABASE_URL is not set)
DB_USER=username
DB_PASSWORD=password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mkulima_smart

# LangChain & OpenAI API Keys
OPENAI_API_KEY=your-openai-api-key
LANGCHAIN_API_KEY=your-langchain-api-key
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=mkulima_smart

# Application Host/Port Settings
HOST=0.0.0.0
PORT=5000
